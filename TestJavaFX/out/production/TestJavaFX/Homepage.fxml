<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="366.0" maxWidth="649.0" minHeight="366.0" minWidth="649.0" prefHeight="366.0" prefWidth="649.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HomepageController">
    <children>
      <ImageView fitHeight="366.0" fitWidth="649.0" layoutX="2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@pic.png" />
         </image>
      </ImageView>
      <TextField fx:id="userText" layoutX="62.0" layoutY="42.0" prefHeight="28.0" prefWidth="306.0" promptText="Enter File Name" />
      <Button fx:id="searchButton" layoutX="447.0" layoutY="37.0" mnemonicParsing="false" onAction="#search" prefHeight="32.0" prefWidth="89.0" text="Search">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Button>
      <Label fx:id="userLabel" layoutX="62.0" layoutY="140.0" opacity="0.77" prefHeight="188.0" prefWidth="524.0" style="-fx-background-color: white" text="">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="show" layoutX="62.0" layoutY="90.0" mnemonicParsing="false" onAction="#ShowFiles" prefHeight="39.0" prefWidth="167.0" text="Show Your Files">
         <font>
            <Font name="System Bold Italic" size="19.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
